{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es2015",
      "es2016",
      "es6",
      "es2017"
    ],
    "paths": {
      "src/*": ["./src/*"],
      "component/*": ["./src/component/*"],
      "constants/*": ["./src/constants/*"],
      "data/*": ["./src/data/*"],
      "service/*": ["./src/service/*"],
      "layout/*": ["./src/layout/*"],
      "page/*": ["./src/page/*"]
    },
    "checkJs": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "src/**/*",
    "src/**/*.ts",
    "src/**/*.jss",
    "src/**/*.tsx",
    "src/**/*.jsx",
    "proxy.js"
  ]
}
